rm(list=ls())

data.head  = read.table("data/T3_thermistor_array.csv",sep=",",nrows=1)
thermal.data = read.table("data/T3_thermistor_array.csv",sep=",",skip=1)
thermal.data[,1] = as.POSIXct(thermal.data[,1],format="%m/%d/%Y %H:%M",tz='GMT')
colnames(thermal.data) = data.head

obs.time = thermal.data[,1]
thermal.data = thermal.data[,2:dim(thermal.data)[2]]
thermal.data = thermal.data[,which(colnames(thermal.data)<0)]
thermal.data = thermal.data[,order(colnames(thermal.data))]
nflux = dim(thermal.data)[2]-2

flux = array(0,c(length(obs.time)-2,nflux)

for (idepth in 1:nflux1+1)
{


    dTdt = (thermal.data[1:nflux+2]-thermal.data[1:nflux])/(obs.time[1:nflux+2]-obs.time[1:nflux])


}
